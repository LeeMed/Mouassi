<?xml version="1.0" encoding="utf-8"?>

<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:foregroundTint="#F4EBEB">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg6"
        android:padding="16dp"
        app:layout_anchorGravity="center"
        tools:context=".MainActivity">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="319dp"
            android:layout_height="282dp"
            android:fontFamily="@font/aguafina_script"
            android:text="Welcome To Mouali \n You can add photos \n Learn more about us \n Have a look to our Pictures "
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="#070707"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.365" />

        <Button
            android:id="@+id/buttonback"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:layout_width="51dp"
            android:layout_height="38dp"
            android:background="#07FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:textAlignment="center"
            android:textSize="24sp"
            app:icon="@drawable/ic_baseline_arrow_back_24"
            app:iconTint="#E89F9F"
            app:iconTintMode="multiply"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:rippleColor="#EF1111"
            app:strokeColor="#FFFFFF" />

        <Button
            android:id="@+id/buttonphoto"
            style="@style/Widget.AppCompat.Button.Small"
            android:layout_width="51dp"
            android:layout_height="38dp"
            android:background="#07FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:textSize="24sp"
            app:icon="@drawable/ic_baseline_photo_library_24"
            app:iconTint="#F5FAF9"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.186"
            app:layout_constraintStart_toStartOf="parent" />


        <Button
            android:id="@+id/buttondown"
            style="@style/Widget.AppCompat.Button.Small"
            android:layout_width="51dp"
            android:layout_height="38dp"
            android:background="#07FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:textSize="24sp"
            app:icon="@drawable/ic_baseline_arrow_downward_24"
            app:iconTint="#F5FAF9"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.775"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/buttonus"
            style="@style/Widget.AppCompat.Button.Small"
            android:layout_width="51dp"
            android:layout_height="38dp"
            android:background="#07FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:textSize="24sp"
            app:icon="@drawable/ic_baseline_face_24"
            app:iconTint="#F5FAF9"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.955"
            app:layout_constraintStart_toStartOf="parent" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_baseline_add_24"
            app:backgroundTint="#E89F9F"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            app:layout_constraintVertical_bias="0.928" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.coordinatorlayout.widget.CoordinatorLayout>












